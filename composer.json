{
    "name": "sideso/hablame",
    "description": "Hablame Notification Channel for Laravel 6, 7, 8, 9, 10 and 11",
    "homepage": "https://github.com/sideso/hablame",
    "license": "MIT",
    "keywords": ["sms", "hablame", "hablameco", "colombia", "laravel"],
    "authors": [
        {
            "name": "Carlos Trujillo",
            "email": "ctrujillo@sideso.com.co",
            "homepage": "https://sideso.com.co",
            "role": "CTO"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.4 || ^7.0",
        "illuminate/notifications": "^6.0 || ^7.0 || ^8.0 || ^9.0 || ^10.0 || ^11.0 || ^12.0",
        "illuminate/support": "^6.0 || ^7.0 || ^8.0 || ^9.0 || ^10.0 || ^11.0 || ^12.0",
        "sideso/sms-message": "^1.1.2"
    },
    "autoload": {
        "psr-4": {
            "Sideso\\Hablame\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sideso\\Hablame\\HablameServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
